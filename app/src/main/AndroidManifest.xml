            </intent-filter>
        </activity>

        <activity android:name=".module.views.viewpage2.ui.ViewPage2" />
        <activity android:name=".module.views.tablayout.ui.TabLayoutActivity" />
        <activity android:name=".module.views.imageview.ui.DynamicActivity" />
        <activity android:name="com.base.library.mvvm.template.ui.Demo3Activity" />
