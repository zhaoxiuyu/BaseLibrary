        <item name="colorPrimary">@color/common_purple_500</item>
        <item name="colorPrimaryVariant">@color/common_purple_700</item>
        <item name="colorOnPrimary">@color/common_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/common_teal_200</item>
